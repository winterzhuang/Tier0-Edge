{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**"]
    },
    "build:scripts": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:web": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**"]
    },
    "build:server": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:servicesExpress": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "persistent": true,
      "cache": false
    },
    "dev:web": {
      "dependsOn": ["^build"],
      "persistent": true,
      "cache": false
    },
    "dev:server": {
      "dependsOn": ["^build"],
      "persistent": true,
      "cache": false
    },
    "dev:servicesExpress": {
      "dependsOn": ["^build"],
      "persistent": true,
      "cache": false,
      "inputs": ["src/**/*.ts", "tsdown.config.ts"]
    },
    "start": {
      "persistent": true,
      "cache": false
    },
    "intl:once": {
      "persistent": true,
      "cache": false
    },
    "intl:watch": {
      "persistent": true,
      "cache": false
    },
    "properties:convert": {
      "persistent": true,
      "cache": false
    }
  }
}
